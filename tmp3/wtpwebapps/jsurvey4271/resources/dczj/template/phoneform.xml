    <description>在线调查html表单样式</description>
    <stylecode><![CDATA["<!--unitname=样式区--><style>
input[type="button"], input[type="submit"], input[type="reset"] {
	-webkit-appearance: none;
	border-radius: 5px;
}
#title_top img {width:100%;}
.phoneansw_img{
	width:40%;
}
.messagebox{
   color : red;
}
.clearoption{
   margin-left:5px;
   color: #999999;
   font-size:14px; 
   font-family:微软雅黑;
   cursor:pointer;
   display:none;
}
.clearoption:hover {
  color: #ffffff;
}
</style>
<!--/unitname=样式区-->
<!--unitname=题目区-->
<div id = "surveyPaging0" >
<!--for name="问题"-->
<table id="<!--tableid-->" width="100%" style="margin-top:10px;">
  <tr>
    <td style="word-break:break-all;width:100%; margin:0px auto;padding:8px 10px 0px 10px;"><span style="color:#333;font-size:16px; font-family:微软雅黑;line-height:22px;"><!--number--><!--title--><!--qmustfill--></span><!--clearoption--></td>
  </tr>
  <!--jsurveyquesnote-->
  <tr><td><span id="<!--messageid-->" class="messagebox"></span></td></tr>
  <tr>
    <td>
      <table width="100%" style="margin-bottom:10px;margin-top:15px; padding:10px 10px;word-break:break-all;background:#FFFFFF;border:1px solid #e0e0e0;border-radius:5px;">
        <!--for name="答案"-->
        <tr class="survey_listbg">
          <td style="height:40px;font-size:16px;font-family:微软雅黑;"><!--option--><!--answ_img--><!--title--></td>
        </tr>
        <!--/for-->
      </table>
    </td>
  </tr>
</table>
<!--/for--> 
</div>
<!--/unitname=题目区-->
<!--unitname=用户信息区-->
<div id="userform" style="padding:10px 2px;">
  <div style="width:100%; margin:0px auto;background:#FFFFFF;border:1px solid #e0e0e0;border-radius:5px;">
  <!--userbasemessage-->
  <!--phonecode-->
  <table width='100%' border='0' cellspacing='0' cellpadding='0' style='margin-bottom:20px;margin-top:20px; padding:0px 0px;'>
  	<tbody><tr>
  		<td  width='25%' align='right' style='font-size: 15px; color: #333;'><!--validatecodename--></td>
  		<td width='75%'><!--validatecode--></td>
  	</tr>
  	<tr>
  		<td></td><td><span id="validcode" class="msg-box"></span></td>
  	</tr>
  	</tbody></table>
  </div>
</div>
<div style="font-size:15px;font-family:微软雅黑;color:red;padding: 5px 5px;">
                  <span id="errormessage"></span>
</div>
<!--/unitname=用户信息区-->
<!--unitname=提交区-->
<div id="surveySubmitBtn" style="padding: 5px 5px 5px 5px;">
	<button id="surveySubmit" class="important-button" type="button" onclick="phonesurveysubmitvalidate();">提交</button>
</div>
<div id="surveyReplayBtn" style="padding: 5px 5px 5px 5px;">
	<button class="important-button2" type="reset" onclick="qingkong();">重新填写</button>
</div>
<!--/unitname=提交区-->
"]]></stylecode>
